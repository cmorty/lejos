continue
}

debug "Checking condition $cid - [$cid title]" $cid
::set result [$cid check 0]

if {!$result} {
debug "Condition failed"
::set return 0
lappend failures $cid
if {$operator eq "AND"} {
break
}
} else {
debug "Condition passed"
if {$operator eq "OR"} {
::set return 1
break
}
}
}
}
