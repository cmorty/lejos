global info

set next [$info(Wizard) step next]

if {![string length $next]} {
debug "No more panes left to move forward... exiting"
::InstallJammer::actions::Exit $obj
} else {
debug "Moving forward to $next"
after 0 [list $info(Wizard) next 1]
