<!--
NXTLoader is a ROS package included in the stack nxt_lejos_ros which 
allow ROS developers and LeJOS developers to manage your NXT brick
from a ROS Environment.
-->
<launch>
	<node pkg="rosjava_bootstrap" 
	type="run.py" 
	args="NXTLoader org.lejos.ros.nodes.loader.NXTLoader" 
	name="NXTLoader" 
	output="screen" />     
</launch>

